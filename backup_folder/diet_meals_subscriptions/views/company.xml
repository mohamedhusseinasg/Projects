<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_company_form" model="ir.ui.view">
        <field name="name">res.company.form</field>
        <field name="model">res.company</field>
        <field name="inherit_id" ref="base_whatsapp.view_company_form"/>
        <field name="arch" type="xml">
            <xpath expr="//group[@name='whatsapp_messages']" position="after">
                <group>
                    <field name='wa_diet_meals_subscriptions_start_message'/>
                </group>
                <group>
                    <field name='wa_diet_meals_subscriptions_end_message'/>
                </group>
            </xpath>
        </field>
    </record>
</odoo>
